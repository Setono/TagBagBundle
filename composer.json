{
    "name": "setono/tag-bag-bundle",
    "type": "symfony-bundle",
    "description": "A Symfony bundle that creates a session bag called tag which can be used to inject i.e. script tags on your website",
    "license": "MIT",
    "authors": [
        {
            "name": "Joachim LÃ¸vgaard",
            "email": "joachim@loevgaard.dk"
        }
    ],
    "require": {
        "php": "^7.1.3",
        "symfony/config": "^4.3 || ^5.0",
        "symfony/dependency-injection": "^4.3 || ^5.0",
        "symfony/framework-bundle": "^4.3 || ^5.0",
        "symfony/http-foundation": "^4.3 || ^5.0",
        "thecodingmachine/safe": "^0.1",
        "webmozart/assert": "^1.1"
    },
    "require-dev": {
        "kalessil/production-dependencies-guard": "dev-master",
        "korbeil/phpstan-generic-rules": "^0.2",
        "localheinz/composer-normalize": "^1.3",
        "matthiasnoback/symfony-dependency-injection-test": "^4.0",
        "phpspec/phpspec": "^5.1",
        "phpstan/phpstan": "^0.11",
        "phpstan/phpstan-strict-rules": "^0.11",
        "phpunit/phpunit": "^8.0",
        "roave/security-advisories": "dev-master",
        "sebastian/comparator": "^2.0 || ^3.0",
        "sylius-labs/coding-standard": "^3.1",
        "symfony/templating": "^4.3 || ^5.0",
        "symfony/twig-bundle": "^4.3 || ^5.0",
        "thecodingmachine/phpstan-safe-rule": "^0.1"
    },
    "config": {
        "sort-packages": true
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.1-dev"
        }
    },
    "autoload": {
        "psr-4": {
            "Setono\\TagBagBundle\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Setono\\TagBagBundle\\Tests\\": "tests/"
        }
    },
    "prefer-stable": true,
    "scripts": {
        "analyse": "vendor/bin/phpstan analyse -c phpstan.neon -l max src/",
        "check-style": "vendor/bin/ecs check src/ tests/",
        "fix-style": "vendor/bin/ecs check --fix src/ tests/",
        "phpspec": "vendor/bin/phpspec run",
        "phpunit": "vendor/bin/phpunit",
        "test": [
            "@phpspec",
            "@phpunit"
        ]
    }
}

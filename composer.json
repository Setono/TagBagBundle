{
    "name": "setono/tag-bag-bundle",
    "type": "symfony-bundle",
    "description": "A Symfony bundle that integrates the tag bag library into Symfony",
    "license": "MIT",
    "authors": [
        {
            "name": "Joachim LÃ¸vgaard",
            "email": "joachim@loevgaard.dk"
        }
    ],
    "require": {
        "php": "^7.3",
        "setono/tag-bag": "^1.0",
        "symfony/config": "^4.4 || ^5.0",
        "symfony/dependency-injection": "^4.4 || ^5.0",
        "symfony/event-dispatcher": "^4.4 || ^5.0",
        "symfony/framework-bundle": "^4.4 || ^5.0",
        "symfony/http-foundation": "^4.4 || ^5.0.7",
        "symfony/http-kernel": "^4.4 || ^5.0",
        "thecodingmachine/safe": "^1.0",
        "twig/twig": "^2.0 || ^3.0",
        "webmozart/assert": "^1.1"
    },
    "require-dev": {
        "matthiasnoback/symfony-dependency-injection-test": "^4.1",
        "nyholm/symfony-bundle-test": "^1.6",
        "phpunit/phpunit": "^9.2",
        "roave/security-advisories": "dev-master",
        "setono/code-quality-pack": "^1.0",
        "setono/php-templates-bundle": "^1.0",
        "setono/tag-bag-gtag": "^1.0",
        "setono/tag-bag-php-templates": "^1.0",
        "setono/tag-bag-twig": "^1.0",
        "symfony/twig-bundle": "^4.4 || ^5.0"
    },
    "config": {
        "sort-packages": true
    },
    "extra": {
        "branch-alias": {
            "dev-master": "2.0-dev"
        }
    },
    "autoload": {
        "psr-4": {
            "Setono\\TagBagBundle\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Setono\\TagBagBundle\\Tests\\": "tests/"
        }
    },
    "prefer-stable": true,
    "scripts": {
        "analyse": "vendor/bin/phpstan analyse -c phpstan.neon -l max src/",
        "check-style": "vendor/bin/ecs check src/ tests/",
        "fix-style": "vendor/bin/ecs check --fix src/ tests/",
        "phpspec": "vendor/bin/phpspec run",
        "phpunit": "vendor/bin/phpunit"
    }
}
